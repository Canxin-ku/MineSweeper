<GameWin>:
    id: main_win
    orientation: 'vertical'
    canvas.before:
        Color:
            rgba:(1.0, 0.98, 0.98, 1)
        Rectangle:
            size: self.size
            pos: self.pos

    BoxLayout:
        orientation:'vertical'
        BoxLayout:
            size_hint_y: .1
            spacing: 10
            Label:
                text: ''
                size_hint_x: .05
            Button:
                id: quit
                text: '[color=9400D3]<<[/color]'
                font_size: 35
                markup: True
                background_down: ''
                background_normal: ''
                background_color:(1.0, 0.98, 0.98, 1)
                size_hint_x: .1
                on_release: root.quit_game()
            Label:
                text: ''
                size_hint_x: .5
            Label:
                id: time
                text: '[color=FF0000]00:00[/color]'
                font_size: 35
                markup: True
                size_hint_x: .1
            Label:
                text: ''
                size_hint_x: .05

        Label:
            id: info
            text:'[color=00FF00]Please choose \none function below[/color]'
            markup: True
            font_size: 30
            size_hint_y:.1
        StackLayout:
